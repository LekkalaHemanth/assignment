import java.util.*;
class oddPlace{
public static void main(String args[]){
Scanner sc=new Scanner(System.in);
int n;
System.out.println("Enter size");
n=sc.nextInt();
int a[]=new int[n];
int r[]=new int[n];

int index=1;
int c=0;
for(int i=0;i<n;i++)
{
a[i]=sc.nextInt();
}

for(int i=0;i<n;i++)
{

if(a[i]%2==1) 
{
r[index]=a[i];
index=index+2;
}
else
{
r[c]=a[i];
c=c+2;
}
}
for(int i=0;i<n;i++)
{
System.out.print(" "+r[i]);
}

}
}
